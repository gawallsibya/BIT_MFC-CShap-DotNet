<Window x:Class="WPFControls.FrameSample"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="FrameSample" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="134*" />
            <RowDefinition Height="128*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="142*" />
            <ColumnDefinition Width="136*" />
        </Grid.ColumnDefinitions>
        <Frame Grid.Row="1" Grid.Column="1"  Source="http://www.microsoft.com"></Frame>
        <FlowDocumentReader>
            <FlowDocument>
                <Paragraph>
                    <Frame Source="http://www.microsoft.com"></Frame>
                </Paragraph>
            </FlowDocument>
        </FlowDocumentReader>
    </Grid>
</Window>
