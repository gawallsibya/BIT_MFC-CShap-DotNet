<Application x:Class="PhotoCooperative.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:sbts="clr-namespace:PhotoCooperative"
    Startup="AppStartup"
    >
	<Application.Resources>
		<ObjectDataProvider x:Name="PhotosODP" x:Key="Photos" ObjectType="{x:Type sbts:PhotoList}" />
		<ObjectDataProvider x:Name="ShoppingCartODP" x:Key="ShoppingCart" ObjectType="{x:Type sbts:PrintList}" />
		<ObjectDataProvider x:Name="PrintTypesODP" x:Key="PrintTypes" ObjectType="{x:Type sbts:PrintTypeList}" />
	</Application.Resources>
</Application>
